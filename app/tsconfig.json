{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@utils/*": ["./utils/*"],
      "@test-utils/*": ["./test-utils/*"]
    },
    "jsx": "react",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noResolve": false,
    "noImplicitAny": false,
    "removeComments": true,
    "sourceMap": true,
    "allowJs": true,
    "noEmit": true
  }
}
