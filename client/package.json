{
  "name": "sports",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:inband": "jest --runInBand",
    "test:watch:inband": "jest --watch --runInBand",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "dev": "webpack-dev-server --host=0.0.0.0 --mode=development",
    "build": "webpack --mode=production",
    "postbuild": "cp ./public/index.html ./dist/index.html",
    "start": "npm-run-all --parallel start:*",
    "start:client": "serve --no-clipboard --single --listen 8080 dist",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json|css|html|md)\"",
    "format": "npm run prettier -- --write",
    "check": "npm run check-format && npm run check-types",
    "check-format": "npm run prettier -- --list-different",
    "check-types": "tsc",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet --fix --ignore-path .gitignore .eslintignore",
    "validate": "npm run test:coverage && npm run test:e2e:run",
    "husky": "jest",
    "setup": "npm install && npm run validate"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.+(js|json|jsx|ts|tsx|css|html|md)": [
      "prettier",
      "jest --findRelatedTests"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@babel/runtime": "^7.10.2",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@testing-library/dom": "^7.12.0",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.2.1",
    "@testing-library/user-event": "^11.4.2",
    "@types/jest": "^26.0.0",
    "@types/jest-axe": "^3.2.2",
    "@types/react": "^16.9.36",
    "@types/react-dom": "^16.9.8",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-jest": "^3.0.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "file-loader": "^6.0.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "is-ci-cli": "^2.1.1",
    "jest": "^26.0.1",
    "jest-axe": "^3.4.0",
    "jest-emotion": "^10.0.32",
    "jest-watch-select-projects": "^2.0.0",
    "jest-watch-typeahead": "^0.6.0",
    "lint-staged": "^10.2.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "serve": "^11.3.2",
    "start-server-and-test": "^1.11.0",
    "style-loader": "^1.2.1",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@reach/router": "^1.3.3",
    "axios": "^0.19.2",
    "emotion-theming": "^10.0.27",
    "formik": "^2.1.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "_moduleDirectories": [
    "test-utils"
  ]
}
